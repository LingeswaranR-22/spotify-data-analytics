# Rename this file to .env and fill in your credentials.
SPOTIFY_CLIENT_ID=your_client_id_here
SPOTIFY_CLIENT_SECRET=your_client_secret_here
# Must match the Redirect URI configured in your Spotify app settings
SPOTIFY_REDIRECT_URI=http://localhost:9090/callback
# Optional: your Spotify username for user-auth workflows (not needed for client-credentials)
SPOTIFY_USERNAME=
